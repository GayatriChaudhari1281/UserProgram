<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>UserSignUp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>First</servlet-name>
  <servlet-class>Register</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>First</servlet-name>
  <url-pattern>/Add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>CP</servlet-name>
  <servlet-class>ChangePassword</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>CP</servlet-name>
  <url-pattern>/ChangePass</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>FR</servlet-name>
  <servlet-class>ForgetPassword</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>FR</servlet-name>
  <url-pattern>/JSP/Forget</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>UserLog</servlet-name>
  <servlet-class>LoginUser</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>UserLog</servlet-name>
  <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ForgetPass</servlet-name>
  <jsp-file>/JSP/ForgetPass.jsp</jsp-file>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>ForgetPass</servlet-name>
  <url-pattern>/Forgetpass</url-pattern>
  </servlet-mapping>
</web-app>